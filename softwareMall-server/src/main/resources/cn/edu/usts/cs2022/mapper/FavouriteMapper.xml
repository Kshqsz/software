<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.edu.usts.cs2022.mapper.FavouriteMapper">


    <insert id="favourite">
        insert into favourite(user_id, product_id)
        values (#{userId}, #{productId})
    </insert>
</mapper>